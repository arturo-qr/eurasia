		function CheckLogon() {
			var un = document.getElementById('ctl00_txtusername');
			if (un.value == '') {
				this.divError.style.display = 'block';
				this.divMessage.style.display = 'None';
				return false;
			}
			else {
				var ckrembme = document.getElementById('ctl00_chkrememberme');
				if (ckrembme.checked) {
					setCookie("BellagioRemMe", un.value, 365);
				}
				setCookie("BellagioUserName", un.value, 1);
				window.location = "https://www.Global.net/login.aspx";
			}
		}
	</script>

