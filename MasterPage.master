			var HomeColumn1 = getXMLNodeValue2(response, ('div id="HomeColumn1"'), ('div'));
			if (isValid(HomeColumn1)) {
				HomeColumn1.innerHTML = getXMLNodeValue2(response, ('div id="HomeColumn1"'), ('div'));
			}
			else {
				HomeColumn1.innerHTML = '';
			}
			jQuery('#Detail').fadeIn('slow');
			jQuery('#Detail2').hide();
			jQuery('#Detail3').hide();
			jQuery('#Detail4').hide();
		}
		var f_curFinishFunction2 = function(response) {
			jQuery('#flashvideo').fadeIn('slow');
			var Details22 = document.getElementById('HomeColumn2');
			Details22.innerHTML = getXMLNodeValue2(response, ('div id="HomeColumn2"'), ('div><!-- /Twitter Feed --')); //"<div id=\"HomeGallery\"><div id=\"HomeGalleryNavigation\"></div><div id=\"Detail\">        <div id=\"DetailHeader\"> </div>        <div id=\"DetailCopy\"></div></div>    <!-- Item 2 -->        <div id=\"Detail2\">        <div id=\"DetailHeader\"></div>        <div id=\"DetailCopy\"></div>        <div id=\"DetailMoreInfo\"></div>    </div>    <!-- Item 3 -->    <div id=\"Detail3\">        <div id=\"DetailHeader\"></div>        <div id=\"DetailCopy\">        </div>        <div id=\"DetailMoreInfo class=\"DetailMoreInfo\"></div>  </div>    <!-- Item 4 -->    <div id=\"Detail4\"><br /><br />        <div id=\"DetailHeader\"></div>        <div id=\"DetailCopy\"></div>        <div id=\"DetailMoreInfo\"></div>    </div>    <!-- /ItemEnd -->  </div><div id=\"HomeColumn22\"></div>";
			document.title = getXMLNodeValue(response, ('title'));
	}
		
	function getNewPage(page_url) {
		if(page_url.toString().indexOf("Default.aspx") != -1){
			ajax(page_url, '', f_curFinishFunction2);
		}
		else{
			ajax(page_url, '', f_curFinishFunction);
		}
	}
	</script>

